{
    "name": "ns/util-bundle",
    "type": "symfony-bundle",
    "description": "Various Utilitity Services and Classes",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nathanael David Noblet",
            "email": "nathanael@noblet.ca"
        },
        {
            "name": "Mark Woynarski",
            "email": "mark.woynarski@nobletdesign.com"
        }
    ],
    "require": {
