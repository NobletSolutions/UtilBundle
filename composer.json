{
    "name": "ns/util-bundle",
    "type": "symfony-bundle",
    "description": "Various Utility Services and Classes",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nathanael David Noblet",
            "email": "nathanael@noblet.ca"
        },
        {
            "name": "Mark Woynarski",
            "email": "mark.woynarski@nobletdesign.com"
        }
    ],
    "require": {
        "php":                  ">=7.1",
        "ext-json":             "*",
        "ext-mbstring":         "*",
        "twig/twig":            "^2.0|^3.0",
        "doctrine/persistence": "^1.0|^2.0",
        "doctrine/dbal":        "^2.0",
        "doctrine/orm":         "^1.0|^2.0",
        "symfony/form":         "^4.0|^5.0",
        "symfony/validator":    "^4.0|^5.0",
        "symfony/routing":      "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": { "NS\\UtilBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "NS\\UtilBundle\\Tests\\": "Tests" }
    },
    "require-dev": {
        "liip/rmt": "^1.1",
        "phpunit/phpunit": "^7.0",
        "symfony/http-kernel": "^4.0"
    }
}

